    "prettier-watch": "onchange \"**\" --exclude-path .gitignore -- prettier --write --ignore-unknown {{changed}}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jeremiah-Olatunde/dtms.git"
  },
