    "node": "21.6.0",
    "npm": "10.2.4"
  },
  "description": "repository for the dtms website",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec tsx ./src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist/*",
    "swc": "swc src -d dist --copy-files --strip-leading-paths",
