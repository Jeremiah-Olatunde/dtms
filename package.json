  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.17",
    "@swc/types": "^0.1.6",
    "nodemon": "^3.1.0",
    "onchange": "^7.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-jinja-template": "^1.4.0"
  },
  "dependencies": {
